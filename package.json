{
  "browserslist": {
    "development": [
      "last 2 Chrome versions",
      "last 2 Edge versions",
      "Explorer >= 9",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "iOS >= 9",
      "Android >= 4.4",
      "last 2 ChromeAndroid versions"
    ],
    "production": [
      "last 2 Chrome versions",
      "last 2 Edge versions",
      "Explorer >= 9",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "iOS >= 9",
      "Android >= 4.4",
      "last 2 ChromeAndroid versions"
    ]
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "fixpack": "^2.3.1"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true
    },
    "extends": "eslint:recommended",
    "root": true
  },
  "postcss": {
    "local-plugins": true,
    "map": false,
    "parser": "scss",
    "plugins": {
      "autoprefixer": {}
    },
    "syntax": "postcss-scss"
  },
  "private": true,
  "scripts": {
    "eslint": "eslint",
    "fixpack": "fixpack",
    "lint": "npm run eslint -- ."
  }
}
